<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans
				http://www.springframework.org/schema/beans/spring-beans.xsd
			http://www.springframework.org/schema/context 
				http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="OrgType" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.md.business.impl.org.OrgType_Service"/>				

	<bean id="Org" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.md.business.impl.org.Org_Service"/>				

	<bean id="DocSequence" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.md.business.ext.org.service.DocSequence_Service"/>				

	<bean id="DocSequenceValue" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.md.business.ext.org.service.DocSequenceValue_Service"/>				

	<bean id="OrgHierarchy" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.md.business.impl.org.OrgHierarchy_Service"/>				

	<bean id="OrgHierarchyItem" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.md.business.impl.org.OrgHierarchyItem_Service"/>				

	<bean id="Inventory" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.md.business.impl.org.Inventory_Service"/>				

	<bean id="InventoryLocationType" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.md.business.impl.org.InventoryLocationType_Service"/>				

	<bean id="InventoryLocation" scope="singleton" lazy-init="true"
		class="net.nan21.dnet.module.md.business.impl.org.InventoryLocation_Service"/>				
</beans>
